{
  "extends": "airbnb",
  "rules": {
    "no-console": "off",
    "class-methods-use-this": "off",
    "max-len": ["error", {
      "code": 120,
    }],
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {},
    "parser": "babel-eslint"
  },
  "env": {
    "node": true,
    "es6": true,
  },
}
